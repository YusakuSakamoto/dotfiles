"-------------------------
" End Neobundle Settings.
"-------------------------

"カラースキーマを設定
colorscheme koehler
syntax on
let g:molokai_original = 1
let g:rehash256 = 1
set number
set title                                                      
set ambiwidth=double                                           
set tabstop=4                                                  
set expandtab                                                  
set shiftwidth=4                                               
set smartindent                                                
set list                                                       
#set listchars=tab:»-,trail:-,eol:↲,extends:»,precedes:«,nbsp:% 
set nrformats-=octal                                           
set hidden                                                     
set history=50                                                 
set virtualedit=block                                          
set whichwrap=b,s,[,],<,>                                      
set backspace=indent,eol,start                                 
set wildmenu                                                   
set encoding=utf-8
set clipboard=unnamed,autoselect
syntax on
